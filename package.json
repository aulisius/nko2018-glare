{
  "name": "nko2018-glare",
  "version": "1.0.0",
  "description": "Glare - Node knockout 2018",
  "main": "server.js",
  "scripts": {
    "prestart": "npm run build",
    "start": "node server.js",
    "watch": "cross-env NODE_ENV=development parcel index.html --no-hmr --open",
    "prebuild": "rm -rf public",
    "build": "cross-env NODE_ENV=production parcel build index.html --out-dir public --out-file index.html --no-minify --public-url ./",
    "postbuild": "cp -r assets/ public/assets"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hackbit/nko2018-glare.git"
  },
  "keywords": [
    "glare"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Hackbit/nko2018-glare/issues"
  },
  "homepage": "https://github.com/Hackbit/nko2018-glare#readme",
  "dependencies": {
    "@tensorflow/tfjs": "^0.13.3",
    "vega-embed": "^3.0.0",
    "express": "^4.16.4",
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "babel-preset-env": "~1.6.1",
    "clang-format": "~1.2.2",
    "cross-env": "^5.1.6",
    "parcel-bundler": "~1.10.3",
    "yalc": "~1.0.0-pre.22"
  },
  "browserslist": [
    "> 2%",
    "ie >= 11",
    "not dead"
  ]
}
